    "@babel/preset-env": "^7.11.5",
    "@types/bluebird-global": "^3.5.12",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.8",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^26.0.15",
    "@types/method-override": "0.0.31",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.9",
    "babel-jest": "^26.3.0",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
