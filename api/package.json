{
  "name": "transcoding",
  "version": "3.0.0",
  "description": "Una API RESTFUL con Node.js para manipular contenido multimedia.",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon ./src/index.ts --watch ./",
    "build": "tsc",
    "start": "npm run build && node ./dist/index.js",
    "comment": "Windows set DEBUG, Linux DEBUG",
    "test:custom": "jest",
    "test:unit": "cross-env NODE_ENV=test jest --detectOpenHandles ./src/api/testing/unit/*"
  },
  "keywords": [
    "node",
    "restful",
    "api"
  ],
  "author": "Carlos Garcia",
  "license": "ISC",
  "dependencies": {
    "@expo/spawn-async": "^1.5.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "debug": "^4.1.1",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "formidable": "^1.2.1",
    "helmet": "^4.1.1",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "mz": "^2.7.0",
    "socket.io": "^2.3.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@types/bluebird-global": "^3.5.12",
    "@types/cors": "^2.8.8",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^26.0.15",
    "@types/method-override": "0.0.31",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.9",
    "babel-jest": "^26.3.0",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.1.2"
  }
}
