{
  "name": "transcoding",
  "version": "3.0.0",
  "description": "Una API RESTFUL con Node.js para manipular contenido multimedia.",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon .",
    "build": "tsc",
    "start": "npm run build && node ./dist/index.js",
    "comment": "Windows set DEBUG, Linux DEBUG",
    "test:custom": "jest",
    "test:unit": "cross-env NODE_ENV=test jest --detectOpenHandles ./src/testing/unit/*",
    "test:inte": "cross-env NODE_ENV=test jest --detectOpenHandles ./src/testing/integration/*"
  },
  "keywords": [
    "node",
    "restful",
    "api"
  ],
  "author": "Carlos Garcia",
  "license": "ISC",
  "dependencies": {
    "@expo/spawn-async": "^1.5.0",
    "axios": "^0.21.4",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "debug": "^4.1.1",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "formidable": "^1.2.1",
    "helmet": "^4.1.1",
    "http-status-codes": "^2.1.4",
    "method-override": "^3.0.0",
    "moment-timezone": "^0.5.33",
    "mongodb": "^4.1.2",
    "mongoose": "^6.0.5",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
